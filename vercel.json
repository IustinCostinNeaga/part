{
    "version": 2,
    "builds": [
    {
    "src": "index.js",
    "use": "@vercel/node"
    },
    {
    "src": "static/**/*",
    "use": "@vercel/static"
    }
    ],
    "routes": [
    {
    "src": "/api/(.*)",
    "dest": "/api/$1"
    },
    {
    "src": "/(.*)",
    "dest": "/static/$1"
    }
    ],
    "devCommand": "npm run preview"
   }
   